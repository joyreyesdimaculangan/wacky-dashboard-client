{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [
      {
        "name": "app",
        "installMode": "prefetch",
        "resources": {
          "files": [
            "/favicon.ico",
            "/index.html",
            "/*.css",
            "/*.js"
          ]
        }
      },
      {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
          "files": [
            "/assets/**",
            "/*.(png|jpg)"
          ]
        }
      }
    ],
    "dataGroups": [
      {
        "name": "api",
        "urls": [
          "https://api.example.com/**"
        ],
        "cacheConfig": {
          "strategy": "freshness",
          "maxSize": 100,
          "maxAge": "1d",
          "timeout": "10s"
        }
      }
    ]
  }